<Page
    x:Class="SimulationInsight.RadarCalculator.Views.WaveformPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:userControls="using:SimulationInsight.RadarCalculator.Views"
    mc:Ignorable="d">

    <Grid x:Name="ContentArea">
        <StackPanel>
            <TextBlock Text="{x:Bind ViewModel.GetHeaderString('Image')}"></TextBlock>
            <userControls:TransmitterUserControl 
                x:Name="MyTransmitterUserControl2"
                Grid.Row="0"
                Grid.Column="0"/>
        </StackPanel>
    </Grid>
</Page>
